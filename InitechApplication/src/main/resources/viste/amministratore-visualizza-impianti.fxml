<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AmministratoreVisualizzaImpiantiController">
   <children>
      <Label alignment="CENTER" layoutX="259.0" layoutY="14.0" text="Visualizza Impianti" textAlignment="CENTER">
         <font>
            <Font name="Calibri" size="25.0" />
         </font>
      </Label>
      <TableView fx:id="impiantiTable" layoutX="97.0" layoutY="73.0" prefHeight="254.0" prefWidth="503.0">
         <columns>
            <TableColumn fx:id="idColumn" prefWidth="47.0" text="ID" />
            <TableColumn fx:id="tipologiaColumn" prefWidth="132.0" text="Tipologia" />
            <TableColumn fx:id="prezzoColumn" prefWidth="67.0" text="Prezzo" />
            <TableColumn fx:id="terrenoColumn" prefWidth="150.0" text="Terreno" />
            <TableColumn fx:id="gestioneColumn" prefWidth="106.0" text="Gestione" />
         </columns>
      </TableView>
      <Label fx:id="errorLabel" alignment="CENTER" layoutX="9.0" layoutY="368.0" text="error label" textFill="RED" AnchorPane.leftAnchor="9.0" AnchorPane.rightAnchor="11.0" />
      <Button fx:id="aggiungiButton" layoutX="548.0" layoutY="352.0" mnemonicParsing="false" onAction="#aggiungiAction" text="Aggiungi Impianto" />
   </children>
</AnchorPane>
